@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Contoso Crafts</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
<style>
    body {
        font-family: Arial, sans-serif;
        background-color: #f2f2f2;
    }
</style>
<script>
    // Get the element you want to animate
    const element = document.getElementById('myElement');

    // Define the animation function
    function animate() {
        element.style.opacity = '0.5'; // Set opacity to 0.5
        element.style.transform = 'translateX(100px)'; // Move element 100 pixels to the right
    }

    // Call the animation function
    animate();
</script>
<!DOCTYPE html>
<html>
<head>
    <title>My Website</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            background-color: #f2f2f2;
        }
    </style>
</head>
<body>
    <iframe width="560" height="315" src="https://www.youtube.com/embed/YOHsXMg]C9A" frameborder="0" allowfullscreen></iframe>

    <div id="myElement">Hello, World!</div>

    <script>
        const element = document.getElementById('myElement');

        function animate() {
            element.style.opacity = '0.5';
            element.style.transform = 'translateX(100px)';
        }

        animate();
    </script>
</body>
</html>




